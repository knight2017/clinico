<%= form_tag doctor_login_path do %>
  <div class="form-group">
    <%= label_tag :email %><br>
    <input type="email" name="email" id="email" class="form-control">
  </div><br>
  <div>
    <%= label_tag :password %><br>
    <input type="password" name="password" id="password" class="form-control">
  </div><br>
  <%= submit_tag "Sign In" %>
<% end %>
